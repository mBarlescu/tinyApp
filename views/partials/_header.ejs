<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
    <% if(user_id){ %>
    <h1>hello <%= users[user_id].email %> </h1>
    <form method="GET" action="/register">
      <button>register</button>
    </form>
    <form method ="POST" action ="/logout">
      <button>logout</button>
    </form>
    <% } else { %>
    <form method="GET" action="/login">
      <button>login</button>
    </form>
    <form method="GET" action="/register">
      <button>register</button>
    </form>
    <% } %>
</body>
</html>